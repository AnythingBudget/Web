{
  "name": "Anything Budget",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev --host 0.0.0.0",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "docker:up": "docker compose up -d",
    "db:migrate": "bun prisma migrate dev",
    "db:migrate:deploy": "bun prisma migrate deploy",
    "db:gen": "bun prisma generate --no-hints",
    "db:gen:prod": "bun prisma generate --no-hints",
    "db:studio": "bun prisma studio",
    "db:reset": "bun prisma migrate reset",
    "db:seed": "bun prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@react-router/node": "^7.6.2",
    "@react-router/serve": "^7.6.2",
    "@trpc/client": "^11.3.1",
    "@trpc/server": "^11.3.1",
    "better-auth": "^1.2.9",
    "isbot": "^5.1.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.2",
    "superjson": "^2.2.2",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@react-router/dev": "^7.6.2",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^20.19.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "prisma": "^6.9.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^5.4.19",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
